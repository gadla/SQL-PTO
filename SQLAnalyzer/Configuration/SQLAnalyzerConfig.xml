<SQLAnalyzer>
	<Configuration>
	<SQLNexusPath>C:\SqlNexus\Internal\sqlnexus.exe</SQLNexusPath>
	<ExportFolder>C:\PSSDiagExporter\</ExportFolder>
	<ErrorLogPathFilter>\*Errorlog_All*.txt</ErrorLogPathFilter>
	<MSInfoPathFilter>\*MSINFO32.txt</MSInfoPathFilter>
	<BaseTasksPathFilter>\*SQL_Base_Task_List*.txt</BaseTasksPathFilter>
	<PlanPathFilter>\*.sqlplan</PlanPathFilter>
	<RTDSCPathFilter>\*RDTSC*.OUT</RTDSCPathFilter>
	<MachinePathFilter>\*MachineCheck_XML_Collector*.xml</MachinePathFilter>
	<DBCCPathFilter>\*DBCCMemoryStatus_Startup.OUT</DBCCPathFilter>
	<SystemHealthPathFilter>\*.xel</SystemHealthPathFilter>
	<GenerateCharts>1</GenerateCharts>
	<PerformNexusImport>0</PerformNexusImport>
	<SQLSystemHealthAnalysis>0</SQLSystemHealthAnalysis>
	<LoadMSInfoFromPSSDiag>1</LoadMSInfoFromPSSDiag>
	<LoadErrorLogFromPSSDiag>1</LoadErrorLogFromPSSDiag>
	<LoadBaseTaskListFromPSSDiag>1</LoadBaseTaskListFromPSSDiag>
	<LoadSQLPlans>1</LoadSQLPlans>
	<LoadExtendedEventFiles>1</LoadExtendedEventFiles>
	<LoadRDTSCFromPSSDiag>1</LoadRDTSCFromPSSDiag>
	<LoadMachineCheckFromPSSDiag>0</LoadMachineCheckFromPSSDiag>
	<ProcessDBCCMemoryStatus>1</ProcessDBCCMemoryStatus>
	<GenerateExcelReport>1</GenerateExcelReport>
	<GenerateSQLSummary>1</GenerateSQLSummary>
	<GenerateRelogFiles>1</GenerateRelogFiles>
	<ValidateSourceFolder>1</ValidateSourceFolder>
	<GenerateFindingsReport>1</GenerateFindingsReport>
	<DebugPreference>SilentlyContinue</DebugPreference>
	<PerformCustomImport>1</PerformCustomImport>
	<DropAndCreateDBForCustomImport>1</DropAndCreateDBForCustomImport>
	<CollectUserCustomerInfo>0</CollectUserCustomerInfo>
	<SkipLargeImportFiles>1</SkipLargeImportFiles>
	<SkipLargeImportFileSizeCutoffMB>1024</SkipLargeImportFileSizeCutoffMB>
	<IncludeSecurityAssessment>1</IncludeSecurityAssessment>
	<VAPathFilter>\*.VAOUT</VAPathFilter>
	<SRPathFilter>\*.SROUT</SRPathFilter>
	<SkipErrorLogLoginSuccess>1</SkipErrorLogLoginSuccess>
	<SkipErrorLogBackupLogSuccess>1</SkipErrorLogBackupLogSuccess>
	</Configuration>
</SQLAnalyzer>