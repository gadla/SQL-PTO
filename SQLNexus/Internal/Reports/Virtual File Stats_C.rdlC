      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>IF OBJECT_ID ('DataSet_WaitStats_WaitStatsTop5Categories') IS NOT NULL AND OBJECT_ID ('tbl_SCRIPT_ENVIRONMENT_DETAILS') IS NOT NULL
	begin        
  SELECT [Value]
  FROM tbl_SCRIPT_ENVIRONMENT_DETAILS
