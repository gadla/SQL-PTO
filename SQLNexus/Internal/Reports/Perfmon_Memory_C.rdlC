    <DataSet Name="DataSetMemory">
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CounterValue">
          <DataField>CounterValue</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CounterDateTime">
          <DataField>CounterDateTime</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource_SqlNexus_Private</DataSourceName>
        <CommandText>declare @tbl as table(instname varchar(100) null, CounterValue decimal(38,2), CounterDateTime datetime)
insert into @tbl
select 
case when InstanceName like '%sqlservr%' then 'SQL Server ( Private Bytes )' + InstanceName else InstanceName end as InstName,
cast(CounterValue as decimal(38,2 ))/1000000 as CounterValue, CounterDateTime
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where 
dl.objectname in ('Process') and CounterName = 'Private Bytes' and InstanceName !='_Total'
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime
and instancename in
(select top 10 InstanceName from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where dl.objectname in ('Process') and CounterName = 'Private Bytes' and InstanceName !='_Total'
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime
group by InstanceName
order by AVG(cast (CounterValue as float)) desc)
order by 3 

insert into @tbl
select  'SQL Server ( Total Server Memory ) ' as InstName,
cast(CounterValue as decimal(38,2 ))/1000 as CounterValue, CounterDateTime
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where 
dl.objectname like '%Memory Manager' and CounterName = 'Total Server Memory (KB)' 
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime

insert into @tbl
select  'SQL Server ( Target Server Memory ) ' as InstName,
cast(CounterValue as decimal(38,2 ))/1000 as CounterValue, CounterDateTime
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where 
dl.objectname like '%Memory Manager' and CounterName = 'Target Server Memory (KB)' 
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime

insert into @tbl
select  'SQL Server ( Working Set ) ' + InstanceName as InstName,
cast(CounterValue as decimal(38,2 ))/1000 as CounterValue, CounterDateTime
from counterdata dat inner join counterdetails dl on dat.counterid = dl.counterid 
where 
dl.objectname like '%Memory Manager' and CounterName = 'Working Set' and InstanceName like '%sqlservr%'
and CounterDateTime &gt;=@StartTime and CounterDateTime&lt;=@EndTime

select instname , CounterValue, CounterDateTime
from @tbl
order by 3</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartTime">
            <Value>=Parameters!StartTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
