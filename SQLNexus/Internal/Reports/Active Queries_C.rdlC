  <DataSets>
    <DataSet Name="DataSet_DataAvailable">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>if OBJECT_ID ('tbl_TopNLogicalReadsByQueryHash') is not null and OBJECT_ID ('tbl_TopNDurationByQueryHash') is not null and OBJECT_ID ('tbl_TopNCPUByQueryHash')  is not null
   select 1  as Available
   else
   select 0  as Available</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Available">
          <DataField>Available</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_Running_Queries">
      <Query>
        <DataSourceName>DataSource_test</DataSourceName>
        <CommandText>select top 20  format(datediff (MI, request_start_time, req.runtime), 'N', 'en-us') 'running_duration_minutes', 
req.request_start_time, 'still running' request_end_time ,
req.request_logical_reads, 
format((req.request_cpu_time/1000.00/60.0), 'N', 'en-us') 'cpu_time_minutes', naq.stmt_text from 
tbl_requests req join tbl_NOTABLEACTIVEQUERIES naq on req.session_id = naq.session_id and req.runtime=naq.runtime
where req.runtime =(select max(runtime) from tbl_requests) and naq.runtime =(select max(runtime) from tbl_requests)
order by req.request_cpu_time desc
</CommandText>
      </Query>
      <Fields>
        <Field Name="running_duration_minutes">
          <DataField>running_duration_minutes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="request_start_time">
          <DataField>request_start_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="request_end_time">
          <DataField>request_end_time</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="request_logical_reads">
          <DataField>request_logical_reads</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="cpu_time_minutes">
          <DataField>cpu_time_minutes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="stmt_text">
          <DataField>stmt_text</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
