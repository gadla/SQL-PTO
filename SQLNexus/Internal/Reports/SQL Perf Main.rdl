    <DataSet Name="DataSet_Import_Report_matched">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>
if object_id ('tblNexusInfo') is not null
begin
	declare @ImportVersion nvarchar(100), @ReportVersion nvarchar(100)
	select  @ImportVersion=[Value] from tblNexusInfo where  Attribute =  'Nexus Importer Version'
	select @ReportVersion=[Value] from tblNexusInfo where  Attribute =  'Nexus Report Version'
	select @ImportVersion 'ImportVersion', @ReportVersion 'ReportVersion', cast( case when @ImportVersion = @ReportVersion then 1 else 0  end as bit ) IsImportReportMatched
end
else
begin
	select  cast(null as nvarchar(100)) 'ImportVersion', cast(null as nvarchar(100)) 'ReportVersion', cast(0 as bit ) IsImportReportMatched
end</CommandText>
      </Query>
      <Fields>
        <Field Name="ImportVersion">
          <DataField>ImportVersion</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ReportVersion">
          <DataField>ReportVersion</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="IsImportReportMatched">
          <DataField>IsImportReportMatched</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet_IsLatestDiag">
      <Query>
        <DataSourceName>DataSource_Private</DataSourceName>
        <CommandText>
if object_id ('tbl_DiagInfo') is not null
begin
	if exists (select * from tbl_DiagInfo where DiagVersion &gt;=1001)
	select cast (1 as int) 'IsLatestDiag'
	else
	select cast (0 as int) 'IsLatestDiag'
end
else	
begin
	select cast (0 as int) 'IsLatestDiag'
end</CommandText>
      </Query>
      <Fields>
        <Field Name="IsLatestDiag">
          <DataField>IsLatestDiag</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
